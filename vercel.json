{
    "version": 2,
    "builds": [
      {
        "src": "sipsavour/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb" }
      }
    ],
    "routes": [
      {
        "src": "/static/(.*)",
        "dest": "/static/$1"
      },
      {
        "src": "/media/(.*)",
        "dest": "/media/$1"
      },
      {
        "src": "/(.*)",
        "dest": "sipsavour/wsgi.py"
      }
    ],
    "buildCommand": "python manage.py collectstatic --noinput"
  }